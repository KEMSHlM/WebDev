/* OUTPUT_FORMAT("elf32-h8300") */
OUTPUT_ARCH(h8300h)
ENTRY("_start")

SECTIONS
{
    . = 0x0;

    .vectors : {
        vector.o(.data)
    }

    .text : {
        *(.text)
    }

    .rodata : {
        *(.string)
        *(.rodata)
        *(.rodata.*)
    }

    .data : {
        *(.data)
    }

    .bss : {
        *(.bss)
        *(COMMON)
    }
}
